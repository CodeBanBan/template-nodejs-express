{
  "name": "nodejs-express-template",
  "version": "1.0.0",
  "description": "Template for NodeJS + Express developent",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development node_modules/.bin/nodemon app.js",
    "standard": "node_modules/.bin/standard",
    "test": "NODE_ENV=test node_modules/.bin/mocha --timeout 5000 -- \"tests/**/*.js\"",
    "api:create": "claudia create --name sample-express-lambda --region ap-northeast-1 --handler lambda.handler --deploy-proxy-api --config claudia-api.json --policies claudia-policy/*.json --set-env NODE_ENV=prod",
    "api:update": "claudia update --config claudia-api.json --set-env NODE_ENV=prod",
    "api:destroy": "claudia destroy --config claudia-api.json"
  },
  "keywords": [
    "template",
    "node",
    "nodejs",
    "express"
  ],
  "author": "CodeBanBan",
  "license": "MIT",
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "http-status": "^1.3.1",
    "lodash": "^4.17.11",
    "pino": "^5.11.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "claudia": "^5.4.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "standard": "^12.0.1",
    "supertest": "^3.4.2"
  }
}

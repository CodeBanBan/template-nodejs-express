{
  "name": "nodejs-express-template",
  "version": "1.0.0",
  "description": "Template for NodeJS + Express developent",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development node_modules/.bin/nodemon app.js",
    "standard": "node_modules/.bin/standard",
    "test:prepare": "./tests/prepare-test.sh",
    "test:start": "NODE_ENV=test node_modules/.bin/mocha --exit --timeout 5000 -- \"tests/**/*.js\"",
    "test": "npm run test:prepare; npm run test:start"
  },
  "keywords": [
    "template",
    "node",
    "nodejs",
    "express"
  ],
  "author": "CodeBanBan",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "http-status": "^1.3.1",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.5",
    "pino": "^5.11.1",
    "sequelize": "^4.42.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "standard": "^12.0.1",
    "supertest": "^3.4.2"
  }
}
